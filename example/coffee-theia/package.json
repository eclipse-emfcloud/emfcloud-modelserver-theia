    "build": "tsc && yarn run download-server && yarn run lint",
    "download-server": "cd ./scripts && ./download-server.sh",
    "lint": "tslint -c ../../configs/tslint.json --project ./tsconfig.json",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "backend": "lib/node/backend-module"
    }
  ]
