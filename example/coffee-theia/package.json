  },
  "dependencies": {
    "@theia/core": "next",
    "@theia/workspace": "next",
    "@eclipse-emfcloud/modelserver-theia": "0.7.0"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
